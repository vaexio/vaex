<PARAM arraysize="2" datatype="char" name="QUERY_STATUS" value="OK"/>
<PARAM arraysize="*" datatype="char" name="QUERY" value="">
<DESCRIPTION>SELECT *

  
FROM gaiadr1.gaia_source
JOIN gaiadr1.tmass_best_neighbour
ON gaiadr1.gaia_source.source_id = gaiadr1.tmass_best_neighbour.source_id
JOIN gaiadr1.tmass_original_valid
ON gaiadr1.tmass_best_neighbour.tmass_oid = gaiadr1.tmass_original_valid.tmass_oid

WHERE

gaiadr1.gaia_source.ra &lt; 0.1 and 
gaiadr1.gaia_source.dec &lt; 0.1 and
gaiadr1.gaia_source.ra &gt; -0.1 and
gaiadr1.gaia_source.dec &gt; -0.1</DESCRIPTION>
</PARAM>
